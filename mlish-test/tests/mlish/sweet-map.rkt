define
  sum [lst : (List Int)] â†’ Int
  match lst with
    [] -> 0
    x :: xs ->
      {x + sum(xs)}

define
  map [f : (â†’ X Y)] [lst : (List X)] â†’ (List Y)
  match lst with
    [] -> nil
    x :: xs ->
      cons
        f x
        map f xs

